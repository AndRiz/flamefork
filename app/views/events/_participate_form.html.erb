
    <div id="participate_form">
      <% if current_user.participating?(@event) %>
          <%= render 'unparticipate' %>
      <% else %>
          <% if current_user.maxcheck?(@event) %>
          <%= render 'participate' %>
          <% else %>
              <%= render 'soldout' %>
          <% end %>
       <% end %>
    </div>
